   "source": [
    "from tkinter import *\n",
    "root = Tk()\n",
    "\n",
    "def click():\n",
    "    mylabel=Label(root,text=entry.get())\n",
    "    mylabel.pack()\n",
    "\n",
    "btn= Button(root,text=\"Enter your name\",padx=32,command=click,fg='orange',bg='black')\n",
    "entry=Entry(root)\n",
    "entry.pack()\n",
    "btn.pack()\n",
    "  \n",
    "\n",
    "\n",
    "root.mainloop()"
   ]
  }
